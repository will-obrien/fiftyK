    <validationRules>
        <fullName>Check4Active_Rooms</fullName>
        <active>true</active>
        <description>If there are any active rooms block user from deactivating the venue</description>
        <errorConditionFormula>AND( hasActiveRooms__c,  NOT(Active__c ))</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Cannot deactivate Venue with active rooms.  Please deactivate rooms in this Venue and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_if_rooms_are_empty</fullName>
        <active>true</active>
        <description>Can&apos;t deactivate venue unless rooms are empty</description>
        <errorConditionFormula>AND( hasSeatsAssigned__c ,   NOT( Active__c ) )</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Cannot deactivate venue until ALL candidates have been moved to other venues.</errorMessage>
    </validationRules>
