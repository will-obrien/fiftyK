    <validationRules>
        <fullName>ADA_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Exam_Date__c &lt;=  Last_Date_For_ADA__c</errorConditionFormula>
        <errorDisplayField>Last_Date_For_ADA__c</errorDisplayField>
        <errorMessage>Please note that Last Date for ADA has to be before the Exam Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deferral_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Exam_Date__c &lt;=   Last_Date_To_Apply_For_Deferral__c</errorConditionFormula>
        <errorDisplayField>Last_Date_To_Apply_For_Deferral__c</errorDisplayField>
        <errorMessage>Please note Last Date to Apply for Deferral has to be before the Exam Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exam_Date</fullName>
        <active>true</active>
        <errorConditionFormula>OR (Exam_Date__c  &lt;= Last_Date_For_Early_Registration__c, 
Exam_Date__c &lt;= Last_Date_For_Standard_Registration__c, Exam_Date__c &lt;= Last_Date_For_Late_Registration__c
)</errorConditionFormula>
        <errorMessage>Last Date for Early/Standard/Late Registration has to be before the Exam Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Isarael_Exam_Date</fullName>
        <active>true</active>
        <errorConditionFormula>OR (Exam_Date__c &lt; TODAY(), 
Israel_Exam_Date__c &lt; TODAY(), 
Israel_Exam_Date__c &lt;= Exam_Date__c
)</errorConditionFormula>
        <errorMessage>Please ensure Exam Date and Israel Exam Date have to be future dates. In addition, Israel Exam Date can not be the same as   Exam Date or less.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Location_Change</fullName>
        <active>true</active>
        <errorConditionFormula>Exam_Date__c &lt;=  Last_Date_For_Location_Change__c</errorConditionFormula>
        <errorDisplayField>Last_Date_For_Location_Change__c</errorDisplayField>
        <errorMessage>Please note that last date for the location change has to be before the Exam Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RA_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Exam_Date__c &lt;=  Religious_Alternative_Last_Date__c</errorConditionFormula>
        <errorDisplayField>Religious_Alternative_Last_Date__c</errorDisplayField>
        <errorMessage>Please note that last date for Religious Alternative has to be before the Exam Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Exam_Change</fullName>
        <active>true</active>
        <errorConditionFormula>Exam_Date__c &lt;=  Last_Date_To_Request_Exam_Change__c</errorConditionFormula>
        <errorDisplayField>Last_Date_To_Request_Exam_Change__c</errorDisplayField>
        <errorMessage>Please note that Last Date for Exam Request Change has to be before the Exam Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scholarship_Last_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Exam_Date__c &lt;=  Last_Date_For_Scholarship__c</errorConditionFormula>
        <errorDisplayField>Last_Date_For_Scholarship__c</errorDisplayField>
        <errorMessage>Please note that Last Date for Scholarship has to be before the Exam Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Early_Date</fullName>
        <active>true</active>
        <errorConditionFormula>OR ( Last_Date_For_Early_Registration__c &gt;=  Last_Date_For_Standard_Registration__c , 
Last_Date_For_Early_Registration__c &gt;=   Last_Date_For_Late_Registration__c
)</errorConditionFormula>
        <errorDisplayField>Last_Date_For_Early_Registration__c</errorDisplayField>
        <errorMessage>Please note Last Date for Early Registration has to be before the last date of the Standard or Late Registration</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Standard_Date</fullName>
        <active>true</active>
        <errorConditionFormula>OR ( Last_Date_For_Standard_Registration__c &gt;=  Last_Date_For_Late_Registration__c
)</errorConditionFormula>
        <errorMessage>Please note Last Date for Standard Registration has to be before the Last Date of the Late Registration</errorMessage>
    </validationRules>
